<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <RootNamespace>Google_Bookmarks_Manager_for_GPOs</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>1.0.0.51</AssemblyVersion>
    <FileVersion>1.0.0.51</FileVersion>
    <ApplicationIcon>book.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="book.png" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="DarkTheme.xaml" />
    <Page Remove="LightTheme.xaml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="book.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="book.png" />
    <Resource Include="DarkTheme.xaml" />
    <Resource Include="LightTheme.xaml" />
  </ItemGroup>

</Project>
